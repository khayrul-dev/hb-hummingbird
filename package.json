{
  "name": "hb-hummingbird",
  "version": "0.0.8",
  "type": "module",
  "keywords": [
    "ui components",
    "tailwind",
    "tailwind css",
    "tailwind components",
    "tailwind elements",
    "tailwind library",
    "tailwind sections",
    "tailwind ui"
  ],
  "license": "MIT",
  "main": "lib/esm/main.js",
  "module": "lib/esm/main.js",
  "types": "lib/esm/main.d.ts",
  "style": "dist/hummingbird.min.css",
  "repository": "https://github.com/khayrul-dev/hb-hummingbird",
  "files": [
    "lib",
    "dist"
  ],
  "author": "",
  "contributors": [
    "Khayrul Islam (https://www.linkedin.com/in/khayrul-developer/)"
  ],
  "scripts": {
    "dev": "astro dev",
    "build:astro": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "build:css": "NODE_ENV=development postcss src/style.css -o dist/hummingbird.css",
    "build:css:min": "NODE_ENV=production postcss src/style.css -o dist/hummingbird.min.css",
    "build:js": "tsc && vite build",
    "build:dist": "vite build && npm run build:css && npm run build:css:min",
    "build:lib": "vite build --config vite.lib.config.js"
  },
  "devDependencies": {
    "@astrojs/starlight": "^0.31.1",
    "@astrojs/starlight-tailwind": "^3.0.0",
    "@astrojs/tailwind": "^5.1.4",
    "@rollup/plugin-terser": "^0.4.4",
    "@tailwindcss/postcss": "^4.0.6",
    "@tailwindcss/vite": "^4.0.3",
    "@types/bootstrap": "^5.2.10",
    "@types/node": "^22.13.1",
    "astro": "^5.1.5",
    "autoprefixer": "^10.4.20",
    "bootstrap": "^5.3.3",
    "cssnano": "^7.0.6",
    "postcss-cli": "^11.0.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "vite": "^6.1.0",
    "vite-plugin-dts": "^4.5.0"
  },
  "dependencies": {
    "postcss": "^8.5.2"
  }
}
